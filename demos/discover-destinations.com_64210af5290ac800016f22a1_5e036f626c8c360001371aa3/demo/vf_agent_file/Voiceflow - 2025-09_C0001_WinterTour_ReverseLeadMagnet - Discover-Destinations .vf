  "functions": [
    {
      "id": "684eb308674753d4156007eb",
      "name": "search_knowledge_base",
      "createdByID": 1077441,
      "folderID": null,
      "code": "export default async function main(args) {\n  // Extract input variables from args\n  const { user_question, vf_token } = args.inputVars;\n\n  // Validate that the required input variable is provided\n  if (!user_question || !vf_token) {\n    return {\n      next: { path: 'error' },\n      trace: [{ type: \"debug\", payload: { message: \"Missing required input variables\" } }]\n    };\n  }\n\n  try {\n    const url = \"https://general-runtime.voiceflow.com/knowledge-base/query\";\n    const requestBody = {\n          \"question\": user_question,\n          \"chunkLimit\": 10,\n          \"synthesis\": false\n       };\n    \n    const response = await fetch(url, {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\",\n          Accept: \"application/json\",\n          Authorization: vf_token,\n        },\n        body: JSON.stringify(requestBody),\n      });\n    \n    if (!response.ok) {\n      throw new Error(`HTTP error! status: ${response.status}`);\n    }\n\n    const body = response.json;\n    const answer = []\n    body.chunks.forEach((chunk, index) => {\n      const url = chunk.source.url;\n\n      if (url){\n         answer.push(`<chunk_${index + 1}_context>${chunk.content}</chunk_${index + 1}_context>\\n<chunk_${index + 1}_url>${url}</chunk_${index + 1}_url>`);\n      }\n      else{\n        answer.push(`<chunk_${index + 1}_context>${chunk.content}</chunk_${index + 1}_context>`);\n      }\n    });\n\n    const chunks = answer.join('\\n');\n    \n    return {\n      outputVars: { chunks },\n      next: { path: 'success' },\n    };\n  } catch (error) {\n    return {\n      next: { path: 'error' },\n      trace: [\n        { type: \"debug\", payload: { message: \"Error: \" + error.message } },\n      ]\n    };\n  }\n}",
      "image": "https://cm4-production-assets.s3.amazonaws.com/1744637727599-30659.png",
      "description": "Use this function for all factual questions or clarifications about Discover Destinations'‚Äîits process, policies, fees, destinations, advisor bios, sample itineraries, blog posts, or any other information published on the site.",
      "pathOrder": [
        "684eb308674753d4156007ec",
        "684eb308674753d4156007ed"
      ],
      "createdAt": "2025-09-17T14:46:13.000Z",
      "updatedAt": "2025-09-18T09:26:44.000Z",
      "updatedByID": 1077441
    }
